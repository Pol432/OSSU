/**
 * Negate a 16-bit number
 */

 CHIP Neg16 {
    IN in[16];  // 16-bit input
    OUT out[16];     // Negated number

    PARTS:
    Add16(a=in, b=true, out=sum);
    Not16(in=sum, out=out);
}